//Print numbers from 1 to n
#include <stdio.h>
#include <stdlib.h>
int main()
{
    int n,i=1,count=0;
    printf("Enter the value of n:\n");
    scanf("%d",&n);
    while(i<=n)
    {
        printf("%d  ",i);
        count++;
        if(count==10)
        {
            printf("\n");
            count=0;
        }
        i++;
    }
}
